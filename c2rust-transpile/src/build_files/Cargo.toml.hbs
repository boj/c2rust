[workspace]

[package]
name = "{{crate_name}}"
authors = ["C2Rust"]
version = "0.0.0"
publish = false
edition = "2018"

{{#if main_module~}}
[[bin]]
path = "{{root_rs_file}}"
name = "{{crate_name}}"
{{else~}}
[lib]
path = "{{root_rs_file}}"
crate-type = ["staticlib"]
{{~/if}}

[dependencies]
{{#if c2rust_bitfields~}}c2rust-bitfields = "0.2"{{~/if}}
{{#if f128~}}f128 = "0.2"{{~/if}}
{{#if num_traits~}}num_traits = "0.2"{{~/if}}
libc = "0.2"

{{#if cross_checks~}}
[dependencies.c2rust-xcheck-plugin]
version = "*"

[dependencies.c2rust-xcheck-derive]
version = "*"

[dependencies.c2rust-xcheck-runtime]
version = "*"
features = ["libc-hash", "fixed-length-array-hash"]

[dependencies.c2rust-xcheck-backend-{{cross_check_backend}}]
version = "*"
{{~/if}}
